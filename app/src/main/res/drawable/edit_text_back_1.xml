<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <size android:height="12.5dp"/>
            <solid android:color="@android:color/transparent"/>
        </shape>
    </item>
    <item android:gravity="left" android:height="5dp" android:width="5dp">
        <shape android:shape="oval">
            <corners android:radius="2dp" android:topRightRadius="0dp" android:bottomLeftRadius="0dp" android:bottomRightRadius="0dp"/>
            <solid android:width="1dp" android:color="@android:color/white"/>
        </shape>
    </item>
    <item android:gravity="right" android:height="5dp" android:width="5dp">
        <shape android:shape="oval">
            <corners android:radius="2dp" android:topRightRadius="0dp" android:bottomLeftRadius="0dp" android:bottomRightRadius="0dp"/>
            <solid android:width="1dp" android:color="@android:color/white"/>
        </shape>
    </item>
    <item android:gravity="bottom|right|center_vertical|center_horizontal|center" android:height="5dp" android:width="5dp">
        <shape android:shape="oval">
            <corners android:radius="2dp" android:topRightRadius="0dp" android:bottomLeftRadius="0dp" android:bottomRightRadius="0dp"/>
            <solid android:width="0.5dp" android:color="@android:color/white"/>
        </shape>
    </item>
    <item android:gravity="bottom" android:height="5dp" android:width="5dp">
        <shape android:shape="oval">
            <corners android:radius="2dp" android:topRightRadius="0dp" android:bottomLeftRadius="0dp" android:bottomRightRadius="0dp"/>
            <solid android:width="1dp" android:color="@android:color/white"/>
            <padding android:left="3dp" android:top="3dp" android:right="3dp" android:bottom="3dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <size android:height="0dp" android:width="0dp"/>
            <corners android:radius="2dp" android:topRightRadius="0dp" android:bottomLeftRadius="0dp" android:bottomRightRadius="0dp"/>
            <stroke android:width="0.5dp" android:color="@android:color/white"/>
        </shape>
    </item>
</layer-list>
